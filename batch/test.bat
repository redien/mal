@echo off
setlocal enabledelayedexpansion

:TEST
    set "str=%~1"
    set "str="
    echo !str!
EXIT /B 0

call :TEST abcdef

set LF=^


REM Must have these lines ^
set "WORD_WITH_LF=a!LF!b"
echo !WORD_WITH_LF!

set ""
